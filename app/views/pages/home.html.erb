<p id="notice"><%= notice %></p>

<h1>JEUX</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Image url</th>
      <th>Location</th>
      <th>Condition</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @annonces.each do |page| %>
      <tr>
        <td><%= page.title %></td>
        <td><%= page.description %></td>
        <td><%= page.image_url %></td>
        <td><%= page.location %></td>
        <td><%= page.condition %></td>
        <td><%= page.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>


